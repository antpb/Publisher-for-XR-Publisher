meta {
  name: Upload Plugin Chunk
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/plugin-upload-chunk
  body: json
  auth: none
}

headers {
  Authorization: Bearer {{apiKey}}
  Content-Type: application/json
}

body:json {
  {
    "userId": "{{username}}",
    "pluginName": "{{pluginName}}",
    "fileData": "{{base64ChunkData}}",
    "chunkNumber": 1,
    "totalChunks": 3
  }
}

docs {
  Upload a chunk of the plugin ZIP file
}
